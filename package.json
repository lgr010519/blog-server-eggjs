{
  "name": "eggjs_server",
  "version": "1.0.0",
  "description": "博客后台服务",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "egg": "^2",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-scripts": "^2",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "moment": "^2.29.4",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "autod": "^3",
    "autod-egg": "^1",
    "egg-bin": "^4",
    "egg-ci": "^2",
    "egg-mock": "^4",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-eggjs_server",
    "stop": "egg-scripts stop --title=egg-server-eggjs_server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "16",
    "type": "github"
  },
  "repository": {
    "type": "git",
